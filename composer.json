{
    "name": "goldquality/phpunit-snapshots",
    "description": "Snapshot testing with PHPUnit",
    "keywords": [
        "phpunit-snapshots",
        "phpunit",
        "snapshot",
        "assert",
        "testing"
    ],
    "license": "MIT",
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "GoldQuality\\PHPUnitSnapshots\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoldQuality\\PHPUnitSnapshots\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "coduo/php-matcher": ">=5",
        "phpunit/phpunit": ">=9.6"
    }
}
